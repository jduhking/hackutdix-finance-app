generator client {
  provider        = "prisma-client-js"

}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
    id       String    @id @default(auto()) @map("_id") @db.ObjectId
    username String
    password String
    tickers User_Ticker[]
}

model User_Ticker {
    id       String    @id @default(auto()) @map("_id") @db.ObjectId
    userId  String @db.ObjectId
    user    User   @relation(fields: [userId], references: [id])
    ticker String
}

